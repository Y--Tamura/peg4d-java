File         = TopLevel;
TopLevel     = DateTime;
export       = DateTime;

//========================================================================

DateTime     = << Day "/" Month "/" Year ":" Hour ":" Minute ":" Second " " Zone >>;

Day = DIGIT DIGIT;
Month = LETTER LETTER LETTER;
Year = DIGIT DIGIT DIGIT DIGIT;
Hour = DIGIT DIGIT;
Minute = DIGIT DIGIT;
Second = DIGIT DIGIT;
Zone = ("+" / "-") DIGIT DIGIT DIGIT DIGIT;

DIGIT = [0-9];
LETTER = [a-zA-Z];
